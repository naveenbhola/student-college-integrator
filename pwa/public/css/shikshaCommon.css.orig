
/*=====  MEDIA QUERIES  ======*/
@media (max-width: 460px) {
    .slide-menu li a {
        font-size: 0.875rem;
        padding-left: 12px;
    }
    .slide-menu li i {
        font-size: 1rem;
    }
}

@media (max-width: 320px) {
    .slide-menu li i {
        display: none;
    }
}

@media (min-width: 320px) {
     :root,
    html {
        font-size: 16px;
    }
}

@media (min-width: 375px) {
     :root,
    html {
        font-size: 17px;
    }
}
body {
    min-width: 320px;
    background: #f1f2f4;
}

body>img {
