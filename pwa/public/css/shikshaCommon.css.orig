/*footer accordion css*/

.link-box {
    max-height: 0px;
    overflow: hidden;
    padding: 0px 15px;
    transition: max-height 0.2s ease-out;
}

.footer-arr-dwn .active {
    display: block;
}

.active.link-box {
    display: block;
    margin: 3px 0 15px;
    max-height: 100%;
    max-height: min-content;
    padding: 0 15px 10px;
    transition: max-height 0.2s ease-in-out;
}

#stream-links,
#exam-links,
#resources-links,
#abroad-links,
#help-links,
#university-links {
    overflow-y: hidden;
    visibility: hidden;
    max-height: 0;
    margin: 0px;
    padding: 0px;
    transition-property: max-height;
    transition-duration: .5s;
    transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
    list-style-type: none;
}

#stream-links:target,
#exam-links:target,
#resources-links:target,
#abroad-links:target,
#university-links:target,
#help-links:target {
    max-height: 100%;
    display: block;
    margin: 3px 0 15px;
    padding: 0 15px 10px;
    visibility: visible;
}

.main-footer ul li a.open {
    display: inline-block;
    position: absolute;
    top: 0;
    right: 0;
    height: 38px;
    left: 0px;
    text-align: right;
}

#stream-links:target+.close,
#exam-links:target+.close,
#resources-links:target+.close,
#university-links:target+.close,
#abroad-links:target+.close,
#help-links:target+.close {
    display: block;
    position: absolute;
    top: 0;
    right: 0;
    height: 38px;
    left: 0;
    text-align: right;
}

#exam-links:target+.close+.open,
#stream-links:target+.close+.open,
#resources-links:target+.close+.open,
#abroad-links:target+.close+.open,
#help-links:target+.close+.open,
#university-links:target+.close+.open {
    display: none;
}

.main-footer ul li a.close {
    display: none;
}

.terms-anchors>li {
    position: relative;
    padding-right: 60px;
    width: 320px;
    list-style-type: none;
}





/*end of accordion css*/


/***Hamburger css***/


/*----------  HIDE SELECT CHECKBOX  ----------*/

.toggle,
.full-screen-close {
    display: none;
}

.full-screen-close {
    width: 100%;
    height: 100%;
    position: fixed;
    cursor: pointer;
    top: 0;
    left: 0;
}




/*----------  TOGGLE OFF-CANVAS MENU  ----------*/

.toggle:checked+.layer_nav>.menu-container {
    margin-left: 0;
}

select[name="hamburgerStreamsSelect"] {
    display: none;
}

div .layer_nav .menu-container.transZero+.layer_l .full-screen-close,
.toggle-right:checked+.menu-right+.full-screen-close {
    display: block;
    background: rgba(0, 0, 0, .5);
    z-index: 99;
    -webkit-tap-highlight-color:transparent;
}

label#off-right {
    position: fixed;
    left: 0px;
    right: 0px;
}

.toggle-right:checked+.bdl {
    background: red;
}

.menu {
    padding-top: 24px;
}

.toggle-btn,
.close-btn {
    cursor: pointer;
}

.toggle-btn {
    font-size: 2.25rem;
}

.close-btn {
    float: right;
    font-size: 1.5rem;
    padding: 1.5rem;
    color: #ededed;
}

.menu-container,
.content {
    -webkit-transition: margin-left 0.2s ease-in-out;
    transition: margin-left 0.2s ease-in-out
}

.menu-container {
    background: #fff;
    width: 17em;
    backface-visibility: hidden;
    box-shadow: 6px 1px 6px -1px rgba(0, 0, 0, .37);
    -webkit-box-shadow: 6px 1px 6px -1px rgba(0, 0, 0, .37);
    -moz-box-shadow: 6px 1px 6px -1px rgba(0, 0, 0, .37);
    /*transform: translate3d(0px, 0px, 0px);
  -webkit-transform: translate3d(0px, 0px, 0px);
  -moz-transform: translate3d(0px, 0px, 0px);
  -ms-transform: translate3d(0px, 0px, 0px);*/
    float: left;
    height: 100%;
    position: fixed;
    z-index: 9999;
    -webkit-transition: -webkit-transform 0.3s ease-out;
    transition: -webkit-transform 0.3s ease-out;
    -o-transition: transform 0.3s ease-out;
    transition: transform 0.3s ease-out;
    transition: transform 0.3s ease-out, -webkit-transform 0.3s ease-out;
    transform: translateX(-103%);
    -webkit-transform: translateX(-103%);
    -moz-transform: translateX(-103%);
    display: none;
}

.menu-container.show {
    display: block;
}

.menu-container.show.transZero {
    transform: translateX(0);
    -webkit-transform: translateX(0);
    -moz-transform: translateX(0);
}

nav.slide-menu {
    height: 100%;
    position: fixed;
    top: 0px;
    left: 0;
    overflow-y: scroll;
    right: 0;
    width: 17em;
    transition: none;
}

.slide-menu i {
    margin-right: 1rem;
    font-size: 1.5rem;
    vertical-align: middle;
}

.slide-menu li a {
    color: #fff;
    padding: 1.5rem;
    font-size: 1.125rem;
    text-transform: uppercase;
    font-weight: 600;
    display: block;
    transition: background-color .5s ease-in-out;
}

.slide-menu li a:hover {
    background-color: #34495e;
}




