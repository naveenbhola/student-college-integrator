    overflow: hidden;
    transform: translate3d(0,0,0)}
.rippleefect:after{content: '';display: block;
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    pointer-events: none;
    /*background-image: radial-gradient(circle,rgba(255, 255, 255, 0.15) ,transparent 10.01%);*/
    background-repeat: no-repeat;
    background-color: rgba(0, 0, 0, 0.075);
    background-position: 50%;
    transform: scale(10,10);
    opacity: 0;
    transition: transform .5s,opacity 1s;}
.rippleefect:active:after {
        transform: scale(0,0);
        opacity: .2;
        transition: 0s
    }
