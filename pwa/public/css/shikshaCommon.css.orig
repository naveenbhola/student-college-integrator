.footer-arr-dwn,
.footer-arr-up {background-position:-372px -18px;width:16px;height:6px;right:15px;top:19px;position:absolute}

.footer-arr-up {
    background-position: -372px -11px;
}
.mrgnTop {
    margin-top: 10px;
}

.main-footer {
    background: #fff
}

.footer-child {
    font-size: 90%;
