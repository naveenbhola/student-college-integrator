/*tostmsg*/

.report-msg {
    bottom: 0;
    left: 0;
    margin: 0 auto;
    position: fixed;
    right: 0;
    max-width: 90%;
    z-index: 99999999;
    box-shadow: 0 2px 2px rgba(0, 0, 0, 0.2);
    background: rgba(0, 0, 0, 0.8);
    border-radius: 10px;
    padding: 12px 15px;
    color: #fff;
    width: max-content;
    visibility: hidden;
    transition: all .2s ease-in-out;
    -webkit-animation: fadeout 0.5s 2.5s ease-in-out;
    animation: fadeout 0.5s 2.5s ease-in-out;
    transform: translateY(100%);
}

.report-msg.showMe {
    visibility: visible;
    /* Show the snackbar */
    -webkit-animation: fadein 0.5s ease-in-out;
    animation: fadein 0.5s ease-in-out;
    bottom: 30px;
    transform: translateY(0);
}




/* Animations to fade the snackbar in and out */

@-webkit-keyframes fadein {
    from {
        bottom: 0;
        opacity: 0;
    }
    to {
        bottom: 30px;
        opacity: 1;
    }
}

@keyframes fadein {
    from {
        bottom: 0;
        opacity: 0;
    }
    to {
        bottom: 30px;
        opacity: 1;
    }
}

@-webkit-keyframes fadeout {
    from {
        bottom: 30px;
        opacity: 1;
    }
    to {
        bottom: 0;
        opacity: 0;
    }
}

@keyframes fadeout {
    from {
        bottom: 30px;
        opacity: 1;
    }
    to {
        bottom: 0;
        opacity: 0;
    }
}

.report-msg p {
    font-size: 0.875rem;
    line-height: 1.58;
    text-align: center;
}




