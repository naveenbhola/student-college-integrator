}

h1,
h2,
h3,
h4,
strong {
    font-weight: 600;
}

.disable-scroll {
    overflow: hidden;
    position: fixed;
    left: 0;
    right: 0;
}

.hidden-class {
    overflow: hidden;
}

body {
    margin: 0;
}

.header {
    background: #007075;
    width: 100%;
    display: table;
    z-index: 9;
    box-shadow: 0 2px 2px 0 rgba(0, 0, 0, .16), 0 0 0 1px rgba(0, 0, 0, .08);
    height: 40px;
    /*line-height: 1 ;*/
}

.back-col,
.hem-menu {
    background: 0 0;
    border: 0;
    cursor: pointer;
    line-height: 0;
    margin: 0;
