{
  "name": "pwa",
  "version": "1.0.0",
  "description": "PWA Project",
  "main": "index.js",
  "scripts": {
    "dev:server": "webpack --config webpack.config.server.js --watch",
    "dev:client": "webpack --config webpack.config.client.js --watch",
    "dev:start": "nodemon --watch devBundling --exec \"node dist/server.bundle.dev.js\"",
    "devBundling": "npm-run-all --parallel dev:*",
    "build:dev": "webpack --config webpackconfig/webpack.dev.client.js  --progress --profile --colors && webpack --config webpackconfig/webpack.dev.server.js  --progress --profile --colors",
    "build:prod": "webpack --config webpackconfig/webpack.prod.client.js  --progress --profile --colors && webpack --config webpackconfig/webpack.prod.server.js  --progress --profile --colors",
    "build:test1": "webpack --config webpackconfig/webpack.test.client.js --env.NODE_ENV='test1'  --progress --profile --colors && webpack --config webpackconfig/webpack.test.server.js  --env.NODE_ENV='test1' --progress --profile --colors",
    "build:test2": "webpack --config webpackconfig/webpack.test.client.js --env.NODE_ENV='test2'  --progress --profile --colors && webpack --config webpackconfig/webpack.test.server.js  --env.NODE_ENV='test2' --progress --profile --colors",
    "build:test3": "webpack --config webpackconfig/webpack.test.client.js --env.NODE_ENV='test3'  --json > chunks.json && webpack --config webpackconfig/webpack.test.server.js  --env.NODE_ENV='test3' --progress --profile --no-colors",
    "build:test4": "webpack --config webpackconfig/webpack.test.client.js --env.NODE_ENV='test4'  --progress --profile --colors && webpack --config webpackconfig/webpack.test.server.js  --env.NODE_ENV='test4' --progress --profile --colors",
    "build:test5": "webpack --config webpackconfig/webpack.test.client.js --env.NODE_ENV='test5'  --progress --profile --colors && webpack --config webpackconfig/webpack.test.server.js  --env.NODE_ENV='test5' --progress --profile --colors",
    "start": "nodemon --watch server --exec babel-node -- dist/server.bundle.dev.js",
    "test:start": "nodemon --watch server --max-old-space-size=4096 --exec babel-node -- dist/server.bundle.dev.js",
    "build:sw": "webpack --config webpackconfig/webpack.config.sw.js",
    "analyze": "webpack-bundle-analyzer --mode static chunks.json --report public/bundle-analyzer.html && webpack-bundle-analyzer --mode static chunks.json --report reports/bundle-analyzer.html",
    "test": "jest --config jest.config.js --env=jsdom --watch",
    "test:report": "jest --config jest.config.js --env=jsdom --coverage",
    "lint": "eslint application/modules/listing/course/components   -f node_modules/eslint-detailed-reporter/lib/detailed.js -o report/lint-report.html",
    "test-report": "junit-viewer --results=junit.xml --save=testRepost.html"
  },
  "author": "Pranjul",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^7.2.5",
    "axios": "^0.17.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-plugin-universal-import": "^1.4.0",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "ejs": "^2.5.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^4.12.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-useragent": "^1.0.12",
    "file-stream-rotator": "^0.2.0",
    "formsy-react": "^1.1.4",
    "glob": "^7.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit": "^5.2.0",
    "mobile-detect": "^1.4.3",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.4",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "preact": "^8.2.9",
    "preact-compat": "^3.18.0",
    "prop-types": "^15.6.1",
    "querystring": "^0.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-google-charts": "^1.5.5",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-modal": "^3.1.12",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-slick": "^0.24.0",
    "react-universal-component": "^2.8.4",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.88.0",
    "sha1": "^1.1.1",
    "to-string-loader": "^1.1.5",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack-isomorphic-tools": "^3.0.5",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^3.4.1",
    "xunit-viewer": "^5.1.6"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.9",
    "file-loader": "^2.0.0",
    "eslint-detailed-reporter": "^0.7.3",
    "eslint-plugin-css-modules": "^2.10.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-jest": "^22.1.2",
    "file-loader": "^2.0.0",
    "image-webpack-loader": "^4.4.0",
    "isomorphic-style-loader": "^4.0.0",
    "jest": "^23.6.0",
    "jest-html-reporter": "^2.4.2",
    "mini-css-extract-plugin": "^0.4.0",
    "npm-run-all": "^4.1.2",
    "postcss-loader": "^2.0.10",
    "raw-loader": "^0.5.1",
    "redux-devtools": "^3.4.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.1",
    "webpack-file-changer": "^2.0.1",
    "webpack-merge": "^4.1.2"
  }
}
